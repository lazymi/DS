#include <stdio.h>
int main()
{
    int a[100],b[100],A=0,B=0,i,j;
    for(i=0;i<3;i++)
        scanf("%d",&a[i]);
    for(i=0;i<3;i++)
        scanf("%d",&b[i]);
    for(i=0;i<3;i++)
    {
          if(a[i]<b[i])
            B=B+1;
          else if(a[i]>b[i])
            A=A+1;
    }
 printf("%d\t%d",A,B);
}
