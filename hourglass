#include<string.h>
int main()
{
  int i,j,a;
  int ar[10][10],temp=-1000;
  for(i=0;i<6;i++)
  { 
    for(j=0;j<6;j++){
        scanf("%d",&ar[i][j]);
    }
  }
  for(i=0;i<3;i++)
  {
    for(j=0;j<3;j++) 
    {
     a=ar[i][j]+ar[i][j+1]+ar[i][j+2]+ar[i+1][j+1]+ar[i+2][j]+ar[i+2][j+1]+ar[i+2][j+2];
      if(temp<a)
        temp=a;  
    }
  
  }
